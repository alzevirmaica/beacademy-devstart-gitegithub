Algoritmo "NomePesoAltura"

Var   nome: caractere ;
      peso , altura: REAL;



Inicio
    escreva("Digite seu nome: ")
    leia(nome)
    escreva("Digite a altura: ")
    leia(altura)
    escreva("Digite o peso: ")
    leia(peso)
    escreva("Nome: " + nome + " Altura: " , altura ,  " Peso: " , peso, "kg" )


Fimalgoritmo